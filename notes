class CPU < RPS
  def initialize()
    @random = ["rock", "paper", "scissors"]
  end

  def random
    @random
  end

  




class Player < RPS
  def initialize(string)
    @input = string
    
  end

  
end  

@rock = "rock"
@paper = "paper"
@scissors = "scissors"

 def rock
    @rock
  end
  
  def paper
    @paper
  end
  
  def scissors
    @scissors
  end
